            "type": "string"
          }
        },
        "type": "object"
      },
      "User": {
        "properties": {
          "id": {
            "readOnly": true,
            "type": "integer"
          },
          "password": {
            "maxLength": 15,
            "minLength": 5,
            "type": "string"
          },
          "username": {
            "minLength": 4,
            "type": "string"
          }
        },
        "required": [
          "password",
          "username"
        ],
        "type": "object"
      },
      "ValidationError": {
        "properties": {
